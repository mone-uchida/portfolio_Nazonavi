<div class="header-search">
  <p>
    <%= form_with url:address_path, method: :get do |f| %>
      <%= text_field_tag :address, value="東京"  %>
      <%= submit_tag '探す', :name => nil, id: "address-submit" %>
    <% end %>
  </p>
  <p>
    <%= form_with url:search_path, method: :get do |f| %>
      <%= text_field_tag :search, value="フリーワード" %>
      <%= submit_tag '探す', :name => nil, id: "search-submit" %>
    <% end %>
  </p>
</div>
